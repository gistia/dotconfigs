{
  "semi": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "singleQuote": true,
  "bracketSpacing": true,
  "printWidth": 120,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": { "parser": "json", "trailingComma": "none" }
    },
    {
      "files": "*.json",
      "options": { "trailingComma": "none" }
    }
  ]
}
